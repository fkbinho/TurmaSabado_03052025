package laco_repeticao;

public class LacoFor {

	public static void main(String[] args) {
		
		System.out.println("Laço for usando break:");
		for (int i = 0; i < 10; i++) {
			if (i == 4) {
				break;
			}
			System.out.print(i + " ");
		}
		
		System.out.println("Laço for usando continue:");
		for (int i = 0; i < 10; i++) {
			if (i == 4) {
				continue;
			}
			System.out.print(i + " ");
		}

	}

}
